# Make logging directory
if [ -e "/var/log/ericsson/cep-mediation-datagen" ]; then
    echo log directory "/var/log/ericsson/cep-mediation-datagen" already exists
else
	mkdir -p /var/log/ericsson/cep-mediation-datagen
	chown -R dcuser:dc5000 /var/log/ericsson/cep-mediation-datagen
    echo log directory "/var/log/ericsson/cep-mediation-datagen" created
fi
